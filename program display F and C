// Online C compiler to run C program online
#include <stdio.h>

int main() {
// program that display F and C usung # and *
    printf("*****\n");
    printf("*\n");
    printf("*\n");
    printf("****\n");
    printf("*\n");
    printf("*\n");
    printf("*\n");
    
    printf("    #####  \n");
    printf(" ##      ##\n");
    printf("#\n");
    printf("#\n");
    printf("#\n");
    printf("#\n");
    printf(" ##      ##\n");
    printf("   #####\n");
    return 0;
}
